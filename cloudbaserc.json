{
  "envId": "cloud1-8gwnqon533ff90dc",
  "framework": {
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "echo 'No build needed'",
          "outputPath": "."
        }
      },
      "analyze": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": "functions/analyze",
          "functions": [
            {
              "name": "analyze",
              "timeout": 30,
              "runtime": "Nodejs12.16",
              "memorySize": 256,
              "handler": "index.main",
              "installDependency": true,
              "ignore": ["node_modules", "node_modules/**/*", ".git"]
            }
          ]
        }
      }
    }
  }
}